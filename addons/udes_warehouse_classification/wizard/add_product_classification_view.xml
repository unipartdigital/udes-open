<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data>

    <record model="ir.ui.view" id="add_product_classification_form">
      <field name="name">Add Product Classification</field>
      <field name="model">udes_warehouse_classification.add_product_classification</field>
      <field name="type">form</field>
      <field name="arch" type="xml">
        <form string="Upload products">
          <group>
            <field name="file_data" required="1"/>
          </group>
          <footer>
            <button name="upload_products"
                    string="Upload"
                    type="object"/>
            <button special="cancel" string="Cancel"/>
          </footer>
        </form>
      </field>
    </record>

  </data>
</odoo>
